#käyttäjä katsoo päiväkirjamerkintänsä
SELECT entry_date, mood, sleep_hours, notes
FROM DiaryEntries
WHERE user_id = 1;

#lisäys
INSERT INTO Meals (user_id, meal_date, meal_type, calories, description)
VALUES (1, '2024-02-01', 'Lunch', 650, 'Pasta and salad');

#päivitys
UPDATE Symptoms
SET severity = 4, notes = 'Pain increased in the evening'
WHERE symptom_id = 1;

#poistaminen
DELETE FROM DiaryEntries
WHERE entry_id = 3;

